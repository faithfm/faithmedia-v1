{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "require": {
        "php": "^8.1",
        "faithfm/laravel-auth0-pattern": "^4.0.2",
        "faithfm/new-relic-helper": "^1.0.4",
        "faithfm/smart-search-php": "^2.4.1",
        "guzzlehttp/guzzle": "^7.9.3",
        "inertiajs/inertia-laravel": "^2.0.4",
        "itsgoingd/clockwork": "^5.3.4",
        "laravel/framework": "^10.48.29",
        "laravel/nova": "^5.4.4",
        "laravel/tinker": "^2.10.1",
        "owen-it/laravel-auditing": "^13.7.2"
    },
    "require-dev": {
        "spatie/laravel-ignition": "^2.9.1",
        "mockery/mockery": "^1.6.12",
        "nunomaduro/collision": "^7.12",
        "phpunit/phpunit": "^10.5.48",
        "fakerphp/faker": "^1.24.1"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "php-http/discovery": true
        }
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        },
        "classmap": [
            "database/seeds",
            "database/factories"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/faithfm/nova"
        },
        {
            "type": "vcs",
            "url": "https://github.com/faithfm/new-relic-helper"
        }
    ]
}
