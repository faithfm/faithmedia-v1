{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "require": {
        "php": "^8.1",
        "faithfm/laravel-auth0-pattern": "^2.0.1",
        "faithfm/smart-search-php": "^2.1",
        "guzzlehttp/guzzle": "^7.2",
        "itsgoingd/clockwork": "^5.1",
        "laravel/framework": "^10.9",
        "laravel/nova": "^4.0",
        "laravel/tinker": "^2.8",
        "laravel/ui": "^4.2",
        "owen-it/laravel-auditing": "^13.1"
    },
    "require-dev": {
        "spatie/laravel-ignition": "^2.0",
        "mockery/mockery": "^1.4.4",
        "nunomaduro/collision": "^7.0",
        "phpunit/phpunit": "^9.0",
        "fakerphp/faker": "^1.9.1"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        },
        "classmap": [
            "database/seeds",
            "database/factories"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/faithfm/nova"
        },
        {
            "type": "vcs",
            "url": "https://github.com/faithfm/laravel-auth0-pattern.git"
        }
    ]
}
