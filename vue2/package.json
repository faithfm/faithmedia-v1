{
    "private": true,
    "scripts": {
        "dev": "npm run development",
        "development": "mix && sed -i '' 's/\\/fonts\\/vendor\\/\\@mdi\\//\\/vue2\\/fonts\\/vendor\\/\\@mdi\\//g' ../public/vue2/css/app.css",
        "watch": "mix watch",
        "watch-poll": "mix watch -- --watch-options-poll=1000",
        "hot": "mix watch --hot",
        "prod": "npm run production",
        "production": "mix --production && sed -i.bak 's|/fonts/vendor/@mdi/|/vue2/fonts/vendor/@mdi/|g' ../public/vue2/css/app.css"
    },
    "devDependencies": {
        "@mdi/font": "^5.6.55",
        "axios": "^0.21.1",
        "bootstrap": "^4.5.2",
        "cross-env": "^7.0",
        "jquery": "^3.5.1",
        "laravel-mix": "^6.0.49",
        "lodash": "^4.17.20",
        "popper.js": "^1.12",
        "resolve-url-loader": "^5.0.0",
        "sass": "~1.32",
        "sass-loader": "^8.0.0",
        "vue": "^2.6.12",
        "vue-loader": "^15.10.1",
        "vue-template-compiler": "^2.6.12"
    },
    "dependencies": {
        "babel-loader": "^8.1.0",
        "eslint": "^6.8.0",
        "howler": "^2.2.0",
        "lz-string": "^1.4.4",
        "nosleep.js": "^0.9.0",
        "npm": "^8.14.0",
        "vue-router": "^3.6.5",
